{
    "name": "arbory/request-logger",
    "description": "Arbory admin request logger",
    "type": "library",
    "authors": [
        {
            "name": "Martins Ungurs",
            "email": "martins.ungurs@cube.lv"
        }
    ],
    "autoload": {
        "psr-4": {
            "Arbory\\AdminLog\\": "src/"
        }
    },
    "require": {
        "laravel/framework": "^9",
        "arbory/arbory": "^2"
    },
    "require-dev": {
        "cubesystems/static-analysers": "^3.1",
        "orchestra/testbench": "^7.11",
        "psr/simple-cache": "^2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arbory\\AdminLog\\AdminLogServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
